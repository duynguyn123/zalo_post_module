<odoo>

    <record id="inherited_module_form_view" model="ir.ui.view">
        <field name="name">inherited.module.form.view</field>
        <field name="model">marketing.content</field>
        <field name="inherit_id" ref="MarketingContent.view_marketing_content_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="New Tab">
                    <field name="zalo_post"/>
                </page>
            </xpath>
        </field>
    </record>


    <record id="inherited_module_form_viewv2" model="ir.ui.view">
        <field name="name">inherited.module.form.viewv2</field>
        <field name="model">marketing.product</field>
        <field name="inherit_id" ref="MarketingContent.view_marketing_content_product_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//div[1]" position="inside">
                <notebook>
                    <page string="New Tab">
                    <field name="zalo_post"/>
                </page>
                </notebook>
            </xpath>
        </field>
    </record>
